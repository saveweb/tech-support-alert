.swlist-sidebar span.fa, span.fa-stack {height:28px; padding:4px 8px 4px 8px; width:28px; text-align:center; float:left;}
.swlist-sidebar span.swtitle-sidebar {display:table-cell; height:36px; vertical-align: middle;}
